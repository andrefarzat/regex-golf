{
  "name": "heuristica",
  "version": "1.0.0",
  "description": "Unirio",
  "main": "index.js",
  "author": "Andre Farzat",
  "license": "MIT",
  "scripts": {
    "test": "ts-node tests/runner.ts | tap-dot",
    "check-coverage": "nyc --reporter=lcov --reporter=html npm run test && nyc report",
    "test-ci": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "localsearch": "ts-node src/index.ts",
    "copy-instances": "cp -r instances dist",
    "neighborhood": "ts-node src/cli/generate-neighborhood.ts",
    "read-log": "ts-node src/cli/read-log.ts",
    "start": "parcel app/index.html"
  },
  "dependencies": {
    "args": "5.0.1",
    "color": "3.1.0",
    "mersenne-twister": "1.1.0",
    "moment": "2.24.0",
    "regexp-tree": "0.0.85",
    "tap-dot": "2.0.0",
    "ts-node": "8.0.2",
    "tslint": "5.13.1",
    "typescript": "3.3.3333",
    "vue-form": "^4.10.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/args": "3.0.0",
    "@types/mersenne-twister": "1.1.2",
    "@types/node": "11.10.4",
    "alsatian": "2.3.0",
    "coveralls": "3.0.2",
    "nyc": "12.0.2",
    "parcel-bundler": "^1.12.0",
    "source-map-support": "0.5.9",
    "vue": "^2.6.8"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
