#!/usr/bin/env python
import os, sys
from subprocess import call


# Parameters
INSTANCE = 'anchors'
SOLUTION = '[s-w][i-l][b-i]'
LOG_LEVEL = 5


def execute(cmd):
    cmd = list(map(str, cmd))
    call(cmd, shell=os.name == 'nt')


def main():
    cmd = ['npm', 'run', 'neighborhood', '--',
        "--instance", INSTANCE,
        "--solution", SOLUTION,
        "--log-level", LOG_LEVEL]

    execute(cmd)


if __name__ == '__main__':
    main()